{% extends 'base.html.twig' %}

{% block title %}AutreExperience index{% endblock %}

{% block body %}
    <h1>AutreExperience index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for autre_experience in autre_experiences %}
            <tr>
                <td>{{ autre_experience.id }}</td>
                <td>{{ autre_experience.name }}</td>
                <td>
                    <a href="{{ path('autre_experience_show', {'id': autre_experience.id}) }}">show</a>
                    <a href="{{ path('autre_experience_edit', {'id': autre_experience.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('autre_experience_new') }}">Create new</a>
{% endblock %}
